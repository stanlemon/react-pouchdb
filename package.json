{
  "name": "@stanlemon/react-pouchdb",
  "version": "0.1.15",
  "description": "React components for interacting with PouchDB.",
  "keywords": [
    "react",
    "pouchdb",
    "couchdb"
  ],
  "author": "Stan Lemon <stanlemon@users.noreply.github.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:stanlemon/react-pouchdb-document.git"
  },
  "main": "./dist/index.js",
  "files": [
    "dist/"
  ],
  "scripts": {
    "test": "jest",
    "watch": "tsc -d -w",
    "build": "rm -rf ./dist/** && tsc -d",
    "release": "rollup -c",
    "lint": "eslint --ext js,jsx,ts,tsx ./src/ ./example/",
    "example": "webpack-dev-server"
  },
  "browserslist": [
    "last 1 year, not dead",
    "edge > 18",
    "not ie 11",
    "not ie_mob 11",
    "not samsung < 1000",
    "not opera < 1000",
    "not kaios < 1000",
    "not android < 1000",
    "not and_qq < 1000"
  ],
  "jest": {
    "roots": [
      "./src/",
      "./example/"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/out/"
    ],
    "testEnvironment": "enzyme",
    "testEnvironmentOptions": {
      "enzymeAdapter": "react16"
    },
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)?$": "babel-jest"
    }
  },
  "peerDependencies": {
    "pouchdb": "^7.2.2",
    "react": "^16.14.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.5",
    "@babel/preset-typescript": "^7.12.1",
    "@types/enzyme": "^3.10.8",
    "@types/jest": "^26.0.15",
    "@types/lodash.merge": "^4.6.6",
    "@types/pouchdb": "^6.4.0",
    "@types/react": "^16.9.55",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.1.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "html-webpack-plugin": "^4.5.0",
    "jest": "^26.6.3",
    "jest-enzyme": "^7.1.2",
    "pouchdb": "^7.2.2",
    "pouchdb-adapter-memory": "^7.2.2",
    "prettier": "^2.1.2",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "rollup": "^2.33.1",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "typescript": "^4.0.5",
    "wait-for-expect": "^3.0.2",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  }
}
