{
  "name": "@stanlemon/react-pouchdb",
  "version": "0.2.0",
  "description": "React components for interacting with PouchDB.",
  "keywords": [
    "react",
    "pouchdb",
    "couchdb"
  ],
  "author": "Stan Lemon <stanlemon@users.noreply.github.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:stanlemon/react-pouchdb-document.git"
  },
  "main": "./dist/index.js",
  "files": [
    "dist/"
  ],
  "scripts": {
    "test": "jest",
    "watch": "tsc -d -w",
    "build": "rm -rf ./dist/** && tsc -d",
    "lint": "eslint --ext js,jsx,ts,tsx ./src/ ./example/",
    "example": "webpack serve"
  },
  "browserslist": [
    "last 1 year, not dead",
    "edge > 18",
    "not ie 11",
    "not ie_mob 11",
    "not samsung < 1000",
    "not opera < 1000",
    "not kaios < 1000",
    "not android < 1000",
    "not and_qq < 1000"
  ],
  "jest": {
    "roots": [
      "./src/",
      "./example/"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/out/"
    ],
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)?$": "babel-jest"
    }
  },
  "peerDependencies": {
    "pouchdb": "^7.2.2",
    "react": "^17.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/preset-env": "^7.13.12",
    "@babel/preset-react": "^7.12.13",
    "@babel/preset-typescript": "^7.13.0",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@types/jest": "^26.0.21",
    "@types/lodash.merge": "^4.6.6",
    "@types/pouchdb": "^6.4.0",
    "@types/react": "^17.0.3",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-jest": "^24.3.2",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.23.0",
    "html-webpack-plugin": "^5.3.1",
    "jest": "^26.6.3",
    "pouchdb": "^7.2.2",
    "pouchdb-adapter-memory": "^7.2.2",
    "prettier": "^2.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "typescript": "^4.2.3",
    "wait-for-expect": "^3.0.2",
    "webpack": "^5.27.2",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2"
  }
}
