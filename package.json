{
  "name": "@stanlemon/react-pouchdb",
  "version": "0.1.15",
  "description": "React components for interacting with PouchDB.",
  "keywords": [
    "react",
    "pouchdb",
    "couchdb"
  ],
  "author": "Stan Lemon <stanlemon@users.noreply.github.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:stanlemon/react-pouchdb-document.git"
  },
  "main": "./dist/index.js",
  "files": [
    "dist/"
  ],
  "scripts": {
    "test": "jest",
    "watch": "tsc -d -w",
    "build": "rm -rf ./dist/** && tsc -d",
    "lint": "eslint --ext js,jsx,ts,tsx ./src/ ./example/",
    "example": "webpack-dev-server"
  },
  "browserslist": [
    "last 1 year, not dead",
    "edge > 18",
    "not ie 11",
    "not ie_mob 11",
    "not samsung < 1000",
    "not opera < 1000",
    "not kaios < 1000",
    "not android < 1000",
    "not and_qq < 1000"
  ],
  "jest": {
    "roots": [
      "./src/",
      "./example/"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/out/"
    ],
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)?$": "babel-jest"
    }
  },
  "peerDependencies": {
    "pouchdb": "^7.2.2",
    "react": "^17.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.2",
    "@types/jest": "^26.0.19",
    "@types/lodash.merge": "^4.6.6",
    "@types/pouchdb": "^6.4.0",
    "@types/react": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.11.0",
    "@typescript-eslint/parser": "^4.11.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-react": "^7.21.5",
    "html-webpack-plugin": "^4.5.0",
    "jest": "^26.6.3",
    "pouchdb": "^7.2.2",
    "pouchdb-adapter-memory": "^7.2.2",
    "prettier": "^2.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "typescript": "^4.1.3",
    "wait-for-expect": "^3.0.2",
    "webpack": "^4.44.2 <5.0.0",
    "webpack-cli": "^3.3.12 <5.0.0",
    "webpack-dev-server": "^3.11.0 <4.0.0"
  }
